package assignmentprograms;

public class Gradecomparision {

	public static void main(String[] args) {
		
		int grade = 80;
		
		if(grade>=80) {
			System.out.println("A");
		}
		if(grade>=70) {
			System.out.println("B");
		}
		if(grade>=60) {
			System.out.println("C");
		}
		else {
			System.out.println("failed");
		}

	}

}
